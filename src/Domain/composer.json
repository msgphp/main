{
    "name": "msgphp/domain",
    "type": "library",
    "license": "MIT",
    "description": "MsgPhp Base domain layer",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MsgPhp\\Domain\\": ""
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "php": "^7.1.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "ramsey/uuid": "^3.7",
        "simple-bus/message-bus": "^3.0",
        "symfony/phpunit-bridge": "^3.4|^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
