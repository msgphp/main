{
    "name": "msgphp/user",
    "type": "library",
    "license": "MIT",
    "description": "MsgPhp User domain layer",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "MsgPhp\\User\\": ""
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "msgphp/domain": "^1.0"
    },
    "require-dev": {
        "doctrine/collections": "^1.5",
        "doctrine/dbal": "^2.6",
        "doctrine/orm": "^2.5",
        "psr/log": "^1.0",
        "ramsey/uuid": "^3.7",
        "simple-bus/message-bus": "^3.0",
        "symfony/console": "^3.4|^4.0",
        "symfony/phpunit-bridge": "^3.4|^4.0",
        "symfony/security-core": "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0",
        "twig/twig": "^2.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
